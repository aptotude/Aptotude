{
  "name": "aptotude",
  "description": "Apto UI for React",
  "homepage": "https://github.com/aptotude/aptotude",
  "bugs": "https://github.com/aptotude/aptotude/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/aptotude/aptotude.git"
  },
  "scripts": {
    "dev": "yarn storybook:local",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch",
    "test:update": "jest --silent -u",
    "storybook:local": "start-storybook -p 9001 -c .storybook",
    "storybook:deploy": "storybook-to-ghpages",
    "lint": "tslint --project .",
    "typecheck": "tsc --noEmit"
  },
  "license": "MIT",
  "version": "1.0.0",
  "engines": {
    "node": "^10.11",
    "yarn": "^1.3.2"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^4.0.6",
    "@storybook/addon-options": "^4.0.6",
    "@storybook/addon-storysource": "^4.0.9",
    "@storybook/react": "^4.0.6",
    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.9",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-options": "^3.2.2",
    "@types/storybook__react": "^4.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta.6",
    "classnames": "^2.2.6",
    "css-loader": "^1.0.1",
    "jest": "^23.6.0",
    "node-sass": "^4.10.0",
    "react-test-renderer": "^16.6.3",
    "react-testing-library": "^5.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.16.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>test-preprocessor.js"
    },
    "testRegex": "\\.spec.(ts|tsx)$",
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
